RewriteEngine On
RewriteBase /ace/

# If it's a real file/dir, serve it directly (optional but nice)
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# /ace or /ace/ -> index.php?u=/
RewriteRule ^$ index.php?u=/ [QSA,L]

# /ace/anything -> index.php?u=/anything
RewriteRule ^(.+)$ index.php?u=/$1 [QSA,L]
